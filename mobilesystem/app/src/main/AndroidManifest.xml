<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobilesystem">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Main">
        <activity android:name=".activitys.RemoveProduto"></activity>
        <activity android:name=".activitys.RemoveFornecedor" />
        <activity android:name=".activitys.CadastrarFornecedor" />
        <activity android:name=".activitys.CadastrarProduto" />
        <activity android:name=".activitys.Produtos" />
        <activity android:name=".activitys.Fornecedores" />
        <activity android:name=".activitys.MenuSystem" />
        <activity android:name=".activitys.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>