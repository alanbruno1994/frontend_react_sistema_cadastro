<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.registrysystem.mobilesystem">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Main">
        <activity android:name="com.registrysystem.mobilesystem.activitys.RemoveProduto"></activity>
        <activity android:name="com.registrysystem.mobilesystem.activitys.RemoveFornecedor" />
        <activity android:name="com.registrysystem.mobilesystem.activitys.CadastrarFornecedor" />
        <activity android:name="com.registrysystem.mobilesystem.activitys.CadastrarProduto" />
        <activity android:name="com.registrysystem.mobilesystem.activitys.Produtos" />
        <activity android:name="com.registrysystem.mobilesystem.activitys.Fornecedores" />
        <activity android:name="com.registrysystem.mobilesystem.activitys.MenuSystem" />
        <activity android:name="com.registrysystem.mobilesystem.activitys.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>